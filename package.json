{
  "name": "professor-kauri",
  "version": "5.0.0-alpha",
  "type:": "module",
  "description": "Professor Kauri is URPG's multi-purpose Discord, rewritten from the ground up for URPG 2.0",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test": "npm run lint",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pokemon-URPG/professor-kauri.git"
  },
  "keywords": [
    "rpg",
    "text-based",
    "pokemon"
  ],
  "author": "Monbrey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pokemon-URPG/professor-kauri/issues"
  },
  "homepage": "https://github.com/Pokemon-URPG/professor-kauri#readme",
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/node": "^17.0.24",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-marine": "^9.4.1",
    "eslint-plugin-import": "^2.26.0",
    "prisma": "^3.12.0",
    "typescript": "^4.7.0-dev.20220408"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@sentry/node": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "common-tags": "^1.8.2",
    "discord.js": "^14.0.0-dev.1649808570-01a423d",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "string-similarity": "^4.0.4",
    "tsyringe": "^4.6.0",
    "urpg.js": "^2.4.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": false,
      "es2022": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "marine/node"
    ],
    "rules": {
      "comma-dangle": "off",
      "@typescript-eslint/comma-dangle": [
        "error",
        "never"
      ],
      "@typescript-eslint/no-dynamic-delete": "off"
    },
    "ignorePatterns": [
      "dist"
    ]
  }
}
